<%= render partial: "user/navbar" %>

<h1 class="text-3xl"><%= @listing.title %></h1>


<% @listing.pictures_as_thumbnails.each do |p| %>
    <%= link_to image_tag(p), p %>
<% end %>


<%= form_with model: @reservation do |f| %>
    <%= f.collection_select :service, @listing.services, :price, :name %>
<% end %>